		<table id="items-table" class="table table-responsive table-condensed table-hover">
		    <thead>
		      <tr>
		        <th>Item Name</th>
		        <th>Price</th>
		        <th></th>
		      </tr>

		  	</thead>
		  	<tbody>
			<% @items.each do |item| %>
		      <tr>
		        <td><%= item.name %></td>
		        <td><%= item.price %></td>
		      </tr>
			<% end %>
			 </tbody>
		</table>
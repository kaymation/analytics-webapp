<div id="wrap">
      <div class="container">

      

      
  <div class="row">

    <div class="span3">

      <div class="page-header">
        <h3>My Restaurants</h3>
      </div>

      <!-- Nav tabs -->
      <ul class="nav nav-pills nav-stacked">
        <% @listing.each do  |r| %>
        <li> <%= link_to r.name, "/restaurants/#{r.id}/show" %></li>
        <% end %>
        <li><a href="/restaurants/new"><span class="glyphicon glyphicon-plus"></span> Add Restaurant</a></li>
      </ul>
      <br>

    </div>
    
    <hr class="hidden-lg hidden-md">

    <div class="span9">
    	<%= yield %>


    </div>

  </div>


      </div>
    </div>